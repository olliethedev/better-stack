{
  "name": "@olliethedev/better-stack",
  "version": "0.0.1",
  "description": "A composable, plugin-based library for building full-stack applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olliethedev/better-stack.git"
  },
  "keywords": [
    "fullstack",
    "react",
    "nextjs"
  ],
  "author": "olliethedev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olliethedev/better-stack/issues"
  },
  "homepage": "https://github.com/olliethedev/better-stack#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./api": {
      "import": {
        "types": "./dist/api/index.d.ts",
        "default": "./dist/api/index.mjs"
      },
      "require": {
        "types": "./dist/api/index.d.cts",
        "default": "./dist/api/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.mjs"
      },
      "require": {
        "types": "./dist/client/index.d.cts",
        "default": "./dist/client/index.cjs"
      }
    },
    "./context": {
      "import": {
        "types": "./dist/context/index.d.ts",
        "default": "./dist/context/index.mjs"
      },
      "require": {
        "types": "./dist/context/index.d.cts",
        "default": "./dist/context/index.cjs"
      }
    },
    "./plugins/todos": {
      "import": {
        "types": "./dist/plugins/todos/api.d.ts",
        "default": "./dist/plugins/todos/api.mjs"
      },
      "require": {
        "types": "./dist/plugins/todos/api.d.cts",
        "default": "./dist/plugins/todos/api.cjs"
      }
    },
    "./plugins/todos/client": {
      "import": {
        "types": "./dist/plugins/todos/client.d.ts",
        "default": "./dist/plugins/todos/client.mjs"
      },
      "require": {
        "types": "./dist/plugins/todos/client.d.cts",
        "default": "./dist/plugins/todos/client.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "api": [
        "./dist/api/index.d.ts"
      ],
      "client": [
        "./dist/client/index.d.ts"
      ],
      "context": [
        "./dist/context/index.d.ts"
      ],
      "plugins/todos": [
        "./dist/plugins/todos/api.d.ts"
      ],
      "plugins/todos/client": [
        "./dist/plugins/todos/client.d.ts"
      ]
    }
  },
  "dependencies": {
    "@better-db/core": "1.3.37",
    "@olliethedev/yar": "1.1.0",
    "better-call": "1.0.19",
    "zod": "^4.1.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vue": "^3.5.18"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ]
}
